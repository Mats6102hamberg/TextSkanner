datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model DiaryEntry {
  id        String   @id @default(cuid())
  text      String?
  originalText   String?
  translatedText String?
  imageUrl  String?
  createdAt DateTime @default(now())
}
